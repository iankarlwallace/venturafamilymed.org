{
  "name": "venturafamilymed.org",
  "version": "1.0.0",
  "description": "Venturafamilymed.org Website and Testing suite",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iankarlwallace/venturafamilymed.org.git"
  },
  "keywords": [],
  "author": "Ian Wallace",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iankarlwallace/venturafamilymed.org/issues"
  },
  "homepage": "https://github.com/iankarlwallace/venturafamilymed.org#readme",
  "stylelint": {
    "extends": ["/usr/local/share/.config/yarn/global/node_modules/stylelint-config-standard"],
    "rules": {
      "indentation": 2,
      "no-descending-specificity": null,
      "at-rule-empty-line-before": [
        "always",
        {
          "except": ["blockless-after-same-name-blockless"],
          "ignore": ["after-comment", "first-nested"],
          "severity": "warning"
        }
      ],
      "custom-property-empty-line-before": [
        "always",
        {
          "except": ["after-custom-property"],
          "ignore": ["after-comment", "first-nested", "inside-single-line-block"],
          "severity": "warning"
        }
      ],
      "declaration-empty-line-before": [
        "always",
        {
          "except": ["after-declaration"],
          "ignore": ["after-comment", "first-nested", "inside-single-line-block"],
          "severity": "warning"
        }
      ],
      "rule-empty-line-before": [
        "always",
        {
          "ignore": ["after-comment", "first-nested"],
          "severity": "warning"
        }
      ]
    }
  }
}
