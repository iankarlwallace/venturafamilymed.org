{
  "name": "venturafamilymed.org",
  "version": "1.0.0",
  "description": "Venturafamilymed.org Website and Testing suite",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iankarlwallace/venturafamilymed.org.git"
  },
  "keywords": [],
  "author": "Ian Wallace",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iankarlwallace/venturafamilymed.org/issues"
  },
  "homepage": "https://github.com/iankarlwallace/venturafamilymed.org#readme",
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": "2",
    "semi": "false",
    "singleQuote": "true",
    "printWidth": "80"
  },
  "stylelint": {
    "extends": [
      "/usr/local/share/.config/yarn/global/node_modules/stylelint-config-standard",
      "/usr/local/share/.config/yarn/global/node_modules/stylelint-config-prettier"
    ],
    "plugins": [
      "/usr/local/share/.config/yarn/global/node_modules/stylelint-no-browser-hacks/lib"
    ],
    "rules": {
      "block-closing-brace-newline-after": "always",
      "color-no-invalid-hex": true,
      "indentation": 2,
      "property-no-unknown": true,
      "plugin/no-browser-hacks": [true, {
        "browsers": [
          "last 2 versions",
          "ie >=8"
        ]
      }],
      "max-empty-lines": 1,
      "value-keyword-case": "lower",
      "rule-empty-line-before": ["always", {
        "except": ["first-nested"],
        "ignore": ["after-comment"]
      }],
      "no-descending-specificity": null
    }
  }
}